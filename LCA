import java.util.*;

public class LCA {
    public int getLCA(int a, int b) {
        //满二叉树 且每层从左到右标数 根节点为1 
        //克曼段一个节点（除根结点） 的父节点是该节点数字的一半
        while(a != b){
            if(a > b){
                a /= 2;
            } else {
                b /= 2;
            }
        }
        return a;
    }
}
